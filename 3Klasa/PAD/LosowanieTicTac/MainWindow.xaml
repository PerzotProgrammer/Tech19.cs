<Window x:Class="LosowanieTicTac.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LosowanieTicTac"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0" TextAlignment="Center" FontSize="86" Name="Cell00"></TextBlock>
        <TextBlock Grid.Row="0" Grid.Column="1" TextAlignment="Center" FontSize="86" Name="Cell01"></TextBlock>
        <TextBlock Grid.Row="0" Grid.Column="2" TextAlignment="Center" FontSize="86" Name="Cell02"></TextBlock>
        <TextBlock Grid.Row="1" Grid.Column="0" TextAlignment="Center" FontSize="86" Name="Cell10"></TextBlock>
        <TextBlock Grid.Row="1" Grid.Column="1" TextAlignment="Center" FontSize="86" Name="Cell11"></TextBlock>
        <TextBlock Grid.Row="1" Grid.Column="2" TextAlignment="Center" FontSize="86" Name="Cell12"></TextBlock>
        <TextBlock Grid.Row="2" Grid.Column="0" TextAlignment="Center" FontSize="86" Name="Cell20"></TextBlock>
        <TextBlock Grid.Row="2" Grid.Column="1" TextAlignment="Center" FontSize="86" Name="Cell21"></TextBlock>
        <TextBlock Grid.Row="2" Grid.Column="2" TextAlignment="Center" FontSize="86" Name="Cell22"></TextBlock>
        <Button Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="3" Margin="128 16" FontSize="32" Click="RollButton_OnClick">LOSUJ</Button>
    </Grid>
</Window>