<UserControl x:Class="ListaLudowa.UserData"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ListaLudowa"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="400"
             Height="250px"
             Width="250px">
    <Grid Background="#aaaaaa" Margin="10px">
        <Grid.RowDefinitions>
            <RowDefinition Height="25px"></RowDefinition>
            <RowDefinition Height="25px"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" FontSize="15px" TextWrapping="Wrap" Name="PersonName">Imię:</TextBlock>
        <TextBlock Grid.Row="1" FontSize="15px" TextWrapping="Wrap" Name="PersonGender">Płeć:</TextBlock>
        <TextBlock Grid.Row="2" FontSize="15px" TextWrapping="Wrap" Name="PersonAbout">O sobie:</TextBlock>
    </Grid>
</UserControl>