<Window x:Class="BazaSIMC.SearchByName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:BazaSIMC"
        mc:Ignorable="d"
        Title="Szukaj po nazwie" Height="200" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Margin="10px" FontSize="30px">Podaj nazwę miejscowości:</TextBlock>
        <TextBox Grid.Row="1" Name="SearchBox" Margin="10px" FontSize="20px"></TextBox>
        <Button Grid.Row="2" Margin="10px" Click="SearchButton_OnClick">Szukaj</Button>
    </Grid>
</Window>